@(in-package :sys)
@(mdo (find-struct-type 'assembler))
@(bind oc @(keep-if .code %oc-list-builder%.(get)))
@(next "vm.h")
@(collect)
@{copyright}
@(until)

@(end)
@(output "vmop.h")
@{copyright "\n"}

typedef enum vm_op {
@  (repeat :vars (oc))
  @{oc.symbol :filter :upcase} = @{oc.code},
@  (end)
} vm_op_t;
@(end)
